<?xml version="1.0"?>
<bean package="uk.ac.horizon.ug.exploding.db" class="Message" id="ID" primarykey="ID" primarykeygenerator="assigned" table="messages">
	
	<type name="IDType" javatype="java.lang.String" maxlength="20" />

	<property name="ID" type="IDType" />
	
	<property name="playerID" type="IDType" />
		
	<property name="type" javatype="java.lang.String" />
	
	<property name="year" javatype="java.lang.String" />
	
	<property name="title" javatype="java.lang.String" />
	
	<property name="description" javatype="java.lang.String" />
	
	<property name="createTime" javatype="java.lang.Long" />
	
	<property name="handled" javatype="java.lang.Boolean" />
	
	<property name="handledTime" javatype="java.lang.Long" />
	
	<constant name="MSG_MEMBER_CREATED" javatype="java.lang.String" value='"member_created"'/>
	<constant name="MSG_MEMBER_ASSIMILATED" javatype="java.lang.String" value='"member_assimilated"'/>
	<constant name="MSG_MEMBER_ASSIMILATED_OTHER" javatype="java.lang.String" value='"member_assimilated_other"'/>
	<constant name="MSG_MEMBER_DIED" javatype="java.lang.String" value='"member_died"'/>
	<constant name="MSG_TIMELINE_YEAR" javatype="java.lang.String" value='"timeline_year"'/>
	<constant name="MSG_TIMELINE_CONTENT" javatype="java.lang.String" value='"timeline_content"'/>
	<constant name="MSG_PRIORITY_TIMELINE_CONTENT" javatype="java.lang.String" value='"priority_timeline_content"'/>
	<constant name="MSG_TIMELINE_CONTENT_GLOBAL" javatype="java.lang.String" value='"timeline_content_global"'/>
	
</bean>
